/#closedpoint {
1 0 0 setrgbcolor
2.5 0 360 arc 
gsave fill grestore stroke
} def

/#openpoint {
0 0 0 setrgbcolor
2.5 0 360 arc 
gsave fill grestore stroke
} def

/#Bezier {
	/P3y exch def /P3x exch def
	/P2y exch def /P2x exch def
	/P1y exch def /P1x exch def
	/x exch def /y exch def

	newpath
	x y #closedpoint P3x P3y #closedpoint
	P1x P1y #openpoint P2x P2y #openpoint
	stroke
	closepath

	gsave
  	[3 3] 0 setdash
	x y moveto P1x P1y lineto
	P3x P3y moveto P2x P2y lineto
	stroke
	grestore	

	x y moveto
	P1x P1y P2x P2y P3x P3y curveto
	0 1 0 setrgbcolor
} def

0 0 translate
100 100 150 350 200 250 250 100 #Bezier stroke

100 250 translate
100 100 0 330 150 220 200 100 #Bezier stroke

150 300 translate
100 100 200 25 330 110 109 174 #Bezier stroke

-200 0 translate
100 100 250 163 23 171 109 74 #Bezier stroke
